conn.onmessage = function(e){
	result = $.parseJSON(e.data)||'';

	if (result[0] == 'noti'){
		result = result[1];

		if(result[0] == 'msg'){
			$("ul.nav>li#message").attr('class', 'active');
			$("ul.nav>li#message>a").attr('href', '{% url messageList %}'+result[1]['roomId']);
		}
	}
	else if(result[0] == 'mail'){
		return;
	}
	else{
		return;
	}
}